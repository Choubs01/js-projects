{
    "manifest_version": 2,
    "version": "1.0",
    "name": "Tab Tracker",
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "permissions": [
        "tabs"
    ]
}
